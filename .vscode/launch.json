{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run FioriTest2-fioritest2",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/fioritest2-approuter/node_modules/@sap/html5-repo-mock/index.js",
      "args": [
        "--standalone",
        "/nsfioritest2/index.html",
        "--ui5",
        "{\"version\":\".\"}"
      ],
      "cwd": "${workspaceFolder}/fioritest2",
      "env": {
        "PORT": "6004",
        "MOCK_LOOKUP_DIRS": "[\"webapp\"]",
        "run.config": "{\"handlerId\":\"ui5_run_config_handler_id\",\"runnableId\":\"/home/user/projects/FioriTest2/fioritest2\"}"
      },
      "envFile": "${workspaceFolder}/fioritest2/.env1",
      "preLaunchTask": "Build FioriTest2"
    }
  ]
}